    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "          original   low  delta\n",
      "name          5614  5512    102\n",
      "address       5753  5752      1\n",
      "hours         1307  1307      0\n",
      "avg_bill       897   897      0\n"
     ]
    }
   ],
   "source": [
    "columns = ['name', 'address', 'hours', 'avg_bill']\n",
    "low_columns = ['low_name', 'low_address', 'low_hours', 'low_avg_bill']\n",
    "unique_values = pd.DataFrame({\n",
    "    'original': [df[col].nunique() for col in columns],\n",
    "    'low': [df[col].nunique() for col in low_columns]\n",
    "}, index=columns)\n",
    "unique_values['delta'] = unique_values['original'] - unique_values['low']\n",
    "\n",
    "print(unique_values)"
   ]

    "n_districts = len(districts)\n",
    "\n",
    "# –í—ã—á–∏—Å–ª—è–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Å—Ç—Ä–æ–∫ –∏ —Å—Ç–æ–ª–±—Ü–æ–≤ –¥–ª—è —Å–µ—Ç–∫–∏ –≥—Ä–∞—Ñ–∏–∫–æ–≤\n",
    "n_cols = 2\n",
    "n_rows = (n_districts // n_cols) + (1 if n_districts % n_cols != 0 else 0)\n",
    "\n",
    "# –°–æ–∑–¥–∞–µ–º —Å–µ—Ç–∫—É –≥—Ä–∞—Ñ–∏–∫–æ–≤\n",
    "fig, axes = plt.subplots(n_rows, n_cols, figsize=(20, n_rows * 6))\n",
    "fig.suptitle('–°—Ä–µ–¥–Ω–∏–π —á–µ–∫ –ø–æ –∫–∞—Ç–µ–≥–æ—Ä–∏—è–º –∑–∞–≤–µ–¥–µ–Ω–∏–π –≤ –∫–∞–∂–¥–æ–º —Ä–∞–π–æ–Ω–µ', fontsize=16, y=1.02)\n",
    "\n",
    "# –ü—Ä–æ—Ö–æ–¥–∏–º –ø–æ –≤—Å–µ–º —Ä–∞–π–æ–Ω–∞–º –∏ —Å—Ç—Ä–æ–∏–º barplot\n",
    "for i, district in enumerate(districts):\n",
    "    # –û–ø—Ä–µ–¥–µ–ª—è–µ–º –ø–æ–∑–∏—Ü–∏—é –≥—Ä–∞—Ñ–∏–∫–∞ –≤ —Å–µ—Ç–∫–µ\n",
    "    row = i // n_cols\n",
    "    col = i % n_cols\n",
    "    \n",
    "    # –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—É—â—É—é –æ—Å—å\n",
    "    if n_rows == 1:\n",
    "        ax = axes[col]\n",
    "    else:\n",
    "        ax = axes[row, col]\n",
    "    \n",
    "    # –§–∏–ª—å—Ç—Ä—É–µ–º –¥–∞–Ω–Ω—ã–µ –ø–æ —Ä–∞–π–æ–Ω—É\n",
    "    df_district = df[df['district'] == district]\n",
    "    \n",
    "    # –°—Ç—Ä–æ–∏–º barplot\n",
    "    sns.barplot(\n",
    "        data=df_district,\n",
    "        x='category',\n",
    "        y='middle_avg_bill',\n",
    "        ax=ax,\n",
    "        estimator='mean',  # –ú–æ–∂–Ω–æ –∑–∞–º–µ–Ω–∏—Ç—å –Ω–∞ 'median'\n",
    "        ci=None,  # –£–±–∏—Ä–∞–µ–º –¥–æ–≤–µ—Ä–∏—Ç–µ–ª—å–Ω—ã–µ –∏–Ω—Ç–µ—Ä–≤–∞–ª—ã\n",
    "        palette='viridis'\n",
    "    )\n",
    "    \n",
    "    # –ù–∞—Å—Ç—Ä–æ–π–∫–∏ –≤–Ω–µ—à–Ω–µ–≥–æ –≤–∏–¥–∞\n",
    "    ax.set_title(f'–†–∞–π–æ–Ω: {district}')\n",
    "    ax.set_xlabel('–ö–∞—Ç–µ–≥–æ—Ä–∏—è –∑–∞–≤–µ–¥–µ–Ω–∏—è')\n",
    "    ax.set_ylabel('–°—Ä–µ–¥–Ω–∏–π —á–µ–∫')\n",
    "    ax.tick_params(axis='x', rotation=45)\n",
    "    ax.grid(axis='y', linestyle='--', alpha=0.7)\n",
    "\n",
    "# –£–¥–∞–ª—è–µ–º –ø—É—Å—Ç—ã–µ subplots (–µ—Å–ª–∏ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ä–∞–π–æ–Ω–æ–≤ –Ω–µ –∫—Ä–∞—Ç–Ω–æ n_cols)\n",
    "for j in range(i + 1, n_rows * n_cols):\n",
    "    row = j // n_cols\n",
    "    col = j % n_cols\n",
    "    if n_rows == 1:\n",
    "        fig.delaxes(axes[col])\n",
    "    else:\n",
    "        fig.delaxes(axes[row, col])\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  }
